{
    "env": {
      "browser": true,
      "es2020": true,
      "jest": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
       "jsx": true
      }
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "import/extensions": [ 1, {
        "js": "always", "json": "always"
      }]
    },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }

  
